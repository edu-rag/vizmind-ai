# --- GROQ Configuration ---
GROQ_API_KEY="your_groq_api_key_here"

# --- MongoDB Configuration ---
MONGODB_URI="your_mongodb_connection_uri_here"

# --- S3 Configuration ---
S3_ACCESS_KEY_ID="your_s3_access_key_id"
S3_SECRET_ACCESS_KEY="your_s3_secret_access_key"
S3_ENDPOINT_URL="your_s3_endpoint_url" # e.g., https://s3.amazonaws.com or http://minio:9000
S3_BUCKET_NAME="your_s3_bucket_name"
S3_PUBLIC_DOMAIN="your_s3_public_domain" # Optional, e.g., pub-12345.r2.dev for Cloudflare R2

# --- LangSmith Configuration ---
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY="your_langsmith_api_key_here"
LANGSMITH_PROJECT="your_langsmith_project_name_here"

# --- Authentication Configuration ---
JWT_SECRET_KEY="your_super_secret_jwt_key_please_change_this" # Use a strong, random key
ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=1440 # 24 hours, for example

# --- Google OAuth Configuration ---
# This is the Audience of your Google ID Token, typically your Web application's Client ID
# or your Android/iOS client ID if the token comes from a mobile app.
GOOGLE_CLIENT_ID="your_google_oauth_client_id.apps.googleusercontent.com"

# --- Tavily Configuration ---
TAVILY_API_KEY="your_tavily_api_key_here"